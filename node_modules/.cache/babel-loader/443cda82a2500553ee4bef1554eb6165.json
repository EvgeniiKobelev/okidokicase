. <strong>Пермь</strong></p>\n    </div>\n  </div>\n</template>\n<script>\nimport { mapGetters } from \"vuex\";\nexport default {\n  data() {\n    return {\n      avatarImg: null,\n      changeBio: false,\n      name: \"\",\n      date: null,\n      options: {\n        year: \"numeric\",\n        month: \"long\",\n        day: \"numeric\",\n      },\n    };\n  },\n  mounted() {\n    this.avatarImg = this.info.avatarImg;\n    this.name = this.info.name;\n    this.date = new Date(this.info.date).toLocaleString(\"ru\", this.options);\n  },\n  computed: {\n    ...mapGetters([\"info\"]),\n  },\n};\n</script>\n"],"sourceRoot":""}]}