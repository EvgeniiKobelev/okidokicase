ign\">\n            <p>\n              <label>\n                <input name=\"group1\" type=\"radio\" v-model=\"gender\" />\n                <span>Мужской</span>\n              </label>\n            </p>\n            <p>\n              <label>\n                <input name=\"group1\" type=\"radio\" v-model=\"gender\" />\n                <span>Неопределенный</span>\n              </label>\n            </p>\n            <p>\n              <label>\n                <input name=\"group1\" type=\"radio\" v-model=\"gender\" />\n                <span>Женский</span>\n              </label>\n            </p>\n          </div>\n        </div>\n        <div class=\"row valign-wrapper\" v-show=\"steps1\">\n          <div class=\"col m2 offset-m3 right-align\">\n            <h5>Страна:</h5>\n          </div>\n          <div class=\"col offset-m1 left-align\">\n            <select class=\"browser-default\" v-model=\"selected\">\n              <option value=\"none\" disabled selected>Страна</option>\n              <option value=\"RU\">Россия</option>\n            </select>\n          </div>\n        </div>\n        <div class=\"row valign-wrapper\" v-show=\"steps2\">\n          <div class=\"col m2 offset-m3 right-align\">\n            <h5>Город:</h5>\n          </div>\n          <div class=\"col offset-m1 left-align\">\n            <select class=\"browser-default\">\n              <option value=\"\" disabled selected>Город</option>\n              <option value=\"1\">Пермь</option>\n              <option value=\"2\">Москва</option>\n              <option value=\"3\">Нижний-новгород</option>\n            </select>\n          </div>\n        </div>\n      </form>\n      <button\n        class=\"btn waves-effect waves-light orange accent-4\"\n        type=\"submit\"\n        @click.prevent=\"activeProject = true\"\n      >\n        Выбрать человека\n        <i class=\"material-icons right\">send</i>\n      </button>\n    </div>\n  </div>\n  <div v-else class=\"row center\">\n    <div class=\"col m8 offset-m2\">\n      <h5>Добро пожаловать, {{ name }}!</h5>\n      <p>\n        С нами уже <strong>1320</strong> человек из <strong>13</strong> стран\n      </p>\n      <p>Описание</p>\n      <p>\n        Lorem ipsum dolor sit amet consectetur adipisicing elit. Praesentium\n        illum necessitatibus magnam, amet doloremque vero odit incidunt est\n        reprehenderit. Nihil nemo asperiores cum magnam. Error consequuntur\n        optio odio similique voluptatibus consequatur a laborum animi soluta?\n      </p>\n      <p>\n        <label>\n          <input type=\"checkbox\" />\n          <span>С правилами согласен</span>\n        </label>\n      </p>\n      <button\n        class=\"btn waves-effect waves-light auth-submit orange accent-4\"\n        type=\"submit\"\n        @click.prevent=\"activeProject = true\"\n      >\n        Участовать в проекте\n        <i class=\"material-icons right\">send</i>\n      </button>\n    </div>\n  </div>\n</template>\n<script>\nimport messages from \"../utils/messages\";\nimport { mapGetters } from \"vuex\";\nexport default {\n  data() {\n    return {\n      name: \"\",\n      activeProject: false,\n      gender: null,\n      step2: false,\n      step3: false,\n      selected: \"none\",\n    };\n  },\n  mounted() {\n    console.log(this.gender);\n    this.name = this.info.name;\n    if (messages[this.$route.query.message]) {\n      this.$message(messages[this.$route.query.message]);\n    }\n  },\n  computed: {\n    ...mapGetters([\"info\"]),\n    steps1() {\n      if (this.gender !== null) {\n        this.step2 = true;\n      }\n      return this.step2;\n    },\n    steps2() {\n      if (this.selected !== \"none\") {\n        this.step3 = true;\n      }\n      return this.step3;\n    },\n  },\n};\n</script>\n"],"sourceRoot":""}]}