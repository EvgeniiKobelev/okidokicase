 info: {},\n  },\n  mutations: {\n    setInfo(state, info) {\n      state.info = info;\n    },\n    clearInfo(state) {\n      state.info = {};\n    },\n  },\n  actions: {\n    async fetchInfo({ dispatch, commit }) {\n      try {\n        const uid = await dispatch(\"getUid\");\n        const info = (\n          await firebase.database().ref(`/users/${uid}/info`).once(\"value\")\n        ).val();\n        commit(\"setInfo\", info);\n      } catch (error) {\n        commit(\"setError\", error);\n        console.log(error);\n        throw error;\n      }\n    },\n  },\n  getters: {\n    info(state) {\n      return state.info;\n    },\n  },\n};\n"]}]}